---
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout primaryTitle="关于">
  <section class="archive">
    <div class="section-content section-tag">
      <div class="archive-tag">
        <h2 class="tag-header">AppleBlog</h2>
        <div class="tag-post-list">
          <ul>
            <li>一个复刻Apple Newsroom的博客网站</li>
          </ul>
        </div>
      </div>

      <div class="archive-tag">
        <h2 class="tag-header">仓库链接</h2>
        <div class="tag-post-list">
          <ul>
            <li>
              <a href="https://github.com/xieyumc/AppleBlog" target="_blank">Github</a>
            </li>
          </ul>
        </div>
      </div>

    </div>
  </section>
</BaseLayout>
